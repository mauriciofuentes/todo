package tareas;

public class Numeros {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=0;
		System.out.println("Introduzca un numero entero:");
			n=sc.nextInt();
			if(n%2==0);
			System.out.println("es par");
			else
				System.out.println("es impar");
	}

}
